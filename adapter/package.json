{
  "name": "medipact-adapter",
  "version": "1.0.0",
  "description": "MediPact Adapter - Core engine for processing EHR data and submitting to Hedera HCS",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:hcs": "node scripts/test-hcs.js",
    "validate": "node scripts/validate-output.js"
  },
  "keywords": [
    "hedera",
    "hcs",
    "medical-data",
    "anonymization"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@hashgraph/sdk": "^2.68.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "vitest": "^4.0.7"
  }
}
