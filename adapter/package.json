  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@hashgraph/sdk": "^2.68.0",
    "axios": "^1.7.9",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "vitest": "^4.0.7"
  }
