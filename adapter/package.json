{
  "name": "medipact-adapter",
  "version": "2.0.0",
  "description": "MediPact Universal Adapter - Connect to ANY healthcare system and extract ALL FHIR R4 resources",
  "type": "module",
  "main": "src/index-universal.js",
  "scripts": {
    "start": "node src/index-universal.js",
    "start:legacy": "node src/index.js",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
